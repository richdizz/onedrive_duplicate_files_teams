{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.17/MicrosoftTeams.schema.json",
    "manifestVersion": "1.17",
    "version": "1.0.0",
    "id": "${{TEAMS_APP_ID}}",
    "developer": {
        "name": "Teams App, Inc.",
        "websiteUrl": "${{TAB_ENDPOINT}}",
        "privacyUrl": "${{TAB_ENDPOINT}}/index.html#/privacy",
        "termsOfUseUrl": "${{TAB_ENDPOINT}}/index.html#/termsofuse"
    },
    "icons": {
        "color": "color.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "Duplicate File Scan",
        "full": "Duplicate File Scan for Teams"
    },
    "description": {
        "short": "Scans OneDrive for duplicate files and allows user to resolve them",
        "full": "Scans OneDrive for duplicate files and allows user to resolve them in Microsoft Teams"
    },
    "accentColor": "#FFFFFF",
    "bots": [],
    "composeExtensions": [
        {
            "botId": "${{BOT_ID}}",
            "commands": [
                {
                    "id": "runScan",
                    "context": [
                        "compose",
                        "commandBox",
                        "message"
                    ],
                    "description": "Scan for an resolve deplicate files in OneDrive",
                    "title": "Duplicate Files Scan",
                    "type": "action",
                    "semanticDescription": "This command allows a user to scan their OneDrive for duplicate files and resolve duplicates."
                }
            ]
        }
    ],
    "configurableTabs": [],
    "staticTabs": [
        {
            "entityId": "index0",
            "name": "Home",
            "contentUrl": "${{TAB_ENDPOINT}}/index.html#/tab",
            "websiteUrl": "${{TAB_ENDPOINT}}/index.html#/tab",
            "scopes": [
                "personal",
                "groupChat",
                "team"
            ]
        }
    ],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": [
        "${{TAB_HOSTNAME}}",
        "${{BOT_DOMAIN}}",
        "token.botframework.com",
        "cescoscornerguitars.z6.web.core.windows.net"
    ],
    "showLoadingIndicator": false,
    "webApplicationInfo": {
        "id": "${{AAD_APP_CLIENT_ID}}",
        "resource": "api://${{TAB_DOMAIN}}/${{AAD_APP_CLIENT_ID}}"
    }
}