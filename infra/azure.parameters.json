{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "resourceBaseName": {
      "value": "helloworld${{RESOURCE_SUFFIX}}"
    },
    "staticWebAppSku": {
      "value": "Free"
    },
    "botAadAppClientId": {
      "value": "${{BOT_ID}}"
    },
    "botAadAppClientSecret": {
      "value": "${{SECRET_BOT_PASSWORD}}"
    },
    "aadClientId": {
      "value": "${{AAD_APP_CLIENT_ID}}"
    },
    "aadClientSecret": {
      "value": "${{SECRET_AAD_APP_CLIENT_SECRET}}"
    },
    "cosmosConnectionString": {
      "value": "${{COSMOS_CONN_STRING}}"
    },
    "cosmosDbName": {
      "value": "${{COSMOS_DATABASE_NAME}}"
    },
    "cosmosContainerName": {
      "value": "${{COSMOS_CONTAINER_NAME}}"
    },
    "eventGridEndpoint": {
      "value": "${{EG_ENDPOINT}}"
    },
    "eventGridKey": {
      "value": "${{EG_KEY}}"
    },
    "webAppSKU": {
      "value": "B1"
    },
    "botDisplayName": {
      "value": "hello-world-bot-with-tab"
    }
  }
}